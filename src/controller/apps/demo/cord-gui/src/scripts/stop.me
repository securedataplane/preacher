# script to stop the cord gui server
#
PID=$(ps -ef | grep jetty-runner | grep -v grep | awk '{print $2}')
if [ -z "$PID" ]
then
  echo jetty-runner not running
  exit 0
fi
kill $PID
sleep 1

PID=$(ps -ef | grep jetty-runner | grep -v grep | awk '{print $2}')
if [ ! -z "$PID" ]
then
  echo jetty-runner still running ?
else
  echo jetty-runner stopped
fi
