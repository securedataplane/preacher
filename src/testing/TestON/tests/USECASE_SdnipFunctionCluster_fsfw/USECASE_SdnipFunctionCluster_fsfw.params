    <config>
        <peerNum> 3 </peerNum>
        <switchNum> 39 </switchNum>
        <pr64514> 10.0.14.1</pr64514>
        <pr64515> 10.0.15.1</pr64515>
        <pr64516> 10.0.16.1</pr64516>
